{
    "extends": ["plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": ["warn"],
        "no-console": "off",
        "semi": "warn"
    }
}

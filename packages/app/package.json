{
    "name": "@orion/app",
    "version": "1.0.0",
    "dependencies": {
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-query": "2.23.1",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.0",
        "serve": "11.3.2"
    },
    "devDependencies": {
        "@orion/shared": "1.0.0",
        "@testing-library/jest-dom": "5.11.4",
        "@testing-library/react": "11.0.4",
        "@testing-library/user-event": "12.1.7",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@types/react-router-dom": "5.1.6",
        "autoprefixer": "9.8.4",
        "postcss-cli": "7.1.1",
        "react-query-devtools": "2.6.0",
        "tailwindcss": "1.8.12",
        "typescript": "3.7.2"
    },
    "scripts": {
        "build": "react-scripts build",
        "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
        "clean": "rm -rf build",
        "dev": "PORT=4200 CI=true react-scripts start",
        "start": "PORT=4200 serve -s build",
        "test": "react-scripts test --color --watchAll=false --runInBand",
        "test:watch": "react-scripts test --color",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "license": "MIT"
}
